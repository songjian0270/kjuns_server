<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators SYSTEM "../dtd/validation.dtd">
<validators>
    
    <validator target="com.kjuns.controller.SystemController" method="requestUploadToken">
	    <field name="token" dataType="String">
	 	    <rule validate="notEmpty" msg="token不能为空"/>
	    </field>
	    <field name="signature" dataType="String">
			<rule validate="notEmpty" msg="signature不能为空" />
		</field>
    </validator>
    
    <validator target="com.kjuns.controller.SystemController" method="upgrade">
	    <field name="ver" dataType="String">
	 	    <rule validate="notEmpty" msg="版本不能为空"/>
	    </field>
	    <field name="platform" dataType="String">
			<rule validate="notEmpty" msg="平台不能为空" />
		</field>
    </validator>
    
</validators>
